{
    "env": {
        "browser": true,
        "commonjs": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": [
            "error",
            "always"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "comma-dangle": [
            "error",
            "only-multiline"
        ]
    }
}
